<lb corresp="#a1"/><gap reason="lost"/><w>w<gap reason="plain" unit="part"/></w><w>yn<gap reason="plain" unit="part"/></w><gap reason="plain" quantity="1"/><w><gap reason="plain" unit="part"/>r<gap reason="plain" unit="part"/></w><gap reason="lost"/>
