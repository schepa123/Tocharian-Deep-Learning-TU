<lb corresp="#b1"/><gap reason="plain" quantity="2"/><w>m<gap reason="plain" unit="part"/></w><w>śtwāra</w><w>kälymisa</w><w>onolmi</w><gap reason="lost"/>
<lb corresp="#b2"/><gap reason="plain" quantity="1"/><w>mā</w><w>r<supplied>u</supplied>waṃ</w><w>tūsa</w><w>soyäṣṣän-m<supplied>e</supplied></w><w>po</w><w>skwa-śconai<supplied>meṃ</supplied></w><gap reason="plain" quantity="7"/><w>k<supplied>e</supplied>st</w><w>yo<supplied>kaisa</supplied></w><gap reason="lost"/>
<lb corresp="#b3"/><gap reason="plain" quantity="2"/><w>k<gap reason="plain" unit="part"/>e</w><w>ṣañ</w><w>añm</w><w>stām</w><w>yama</w><gap reason="plain" quantity="8"/><w>pe</w><w>tākaṃ-ne</w><gap reason="lost"/>
