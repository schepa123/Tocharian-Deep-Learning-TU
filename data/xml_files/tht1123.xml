<lb corresp="#b1"/><gap reason="lost"/><foreign xml:lang="san"><w><gap reason="plain" unit="part"/>v<gap reason="plain" unit="part"/>dguṇā</w><w>sarveṣāṃ</w></foreign><w>tu</w><w>ramtä</w><w>ke</w><gap reason="lost"/>
<lb corresp="#b2"/><gap reason="lost"/><w>-le</w><w>ste</w><w>po</w><w>āñmtsa</w><w>te</w><w>papāṣorñ<supplied>e</supplied>sa</w><w>k<supplied>al</supplied>l<supplied>aṃ</supplied></w><gap reason="lost"/>
