<lb corresp="#a3"/><supplied unit="syllable" quantity="9"/></l>
               </lg>
               <lg n="4" type="strophe">
                  <l n="a"><supplied unit="syllable" quantity="12"/><gap reason="lost"/><w>ṣñy</w><w>oky</w><w>anac</w><pc>:</pc></l>
