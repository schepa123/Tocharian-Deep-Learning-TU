         <div type="transcription">
            <p>
               <lb corresp="#a1"/>
               <gap reason="lost"/>
               <w><supplied>ṣañ-ā</supplied>ñm</w>
               <w>śānmyate</w>
               <w>snai</w>
               <w>św<supplied>ātsi</supplied></w>
               <gap reason="lost"/>
               <lb corresp="#a2"/>
               <gap reason="lost"/>
               <w><supplied>swa</supplied>ñcaiṃ</w>
               <w>tsakṣīyentär-ne</w>
               <w>śe<milestone unit="w" type="break"/></w>
               <gap reason="lost"/>
               <lb corresp="#a3"/>
               <gap reason="lost"/>
