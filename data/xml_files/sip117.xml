<lb corresp="#a8"/><w>cai</w><w>kwañye</w><w>puwarne</w><w>śāmna</w><pc>।</pc><w>caukile</w><pc>।</pc><w>puluśke</w><pc>।</pc><w>malakke</w><pc>।</pc><w>wrau</w><pc>।</pc><w>cinatewe</w><pc>।</pc>
<w>ka<lb corresp="#a9"/>ccināk</w><w>wrau</w><pc>।</pc><w>kolti</w><pc>।</pc><w>palaṃtte</w><pc>।</pc><w>śiṅkiśke</w><pc>।</pc><w>kercapiśke</w><pc>।</pc><w>ortonāk</w>
