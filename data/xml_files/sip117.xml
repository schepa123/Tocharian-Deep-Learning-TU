            <p>
<lb corresp="#a1"/><num value="21">20-1</num><w>kṣuntsa</w><w>yāśe</w><w>lānti</w><w>paṣ</w><w>pikulne</w><w>oktañce</w><w>ikäṃ</w><w>ṣene</w><w>ypoy</w><w>moko</w><w>wrau</w><w>po</w><w>ypo</w>
<lb corresp="#a2"/><w>yntse</w><w>ārtarsa</w><w>śwāra</w><w>pwārane</w><w>śaumoṣe</w><w>pauśye</w><w>lau</w><w>putkār</w><w>cai</w><w>mokṣarāmṣe</w><w>puwarśā</w>
<lb corresp="#a3"/><w>śāmna</w><pc>।</pc><w>kerentsile</w><pc>।</pc><w>kkercipile</w><pc>।</pc><w>ptumparre</w><pc>।</pc><w>catile</w><pc>।</pc><w>mikkiṣone</w><pc>।</pc><w>ykāṃnā</w><pc>।</pc>
<w>kwa<lb corresp="#a4"/>ppale</w><pc>।</pc><w>śaṅkale</w><pc>।</pc><w>kercapiśke</w><pc>।</pc><w>kepautile</w><pc>।</pc><w>capeśakke</w><pc>।</pc><w>kepautile</w><pc>।</pc><w>citre</w>
<lb corresp="#a5"/><pc>।</pc><w>puttamitre</w><pc>।</pc><w>sotärkāne</w><pc>।</pc><w>mitraiyśe</w><pc>।</pc><w>laraciwe</w><pc>।</pc><w>oktale</w><pc>।</pc><w>puñicaṃndre</w><pc>।</pc><w>saṅkaupte</w>
<lb corresp="#a6"/><pc>।</pc><w>kepautile</w><pc>।</pc><w>tarmasene</w><pc>।</pc><w>kumpāntiśke</w><pc>।</pc><w>kutsawane</w><w>putamitre</w><pc>।</pc><w>palāk<supplied>k</supplied>e</w><pc>।</pc>
