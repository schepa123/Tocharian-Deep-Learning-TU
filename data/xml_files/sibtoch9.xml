            <p>
<lb corresp="#a1"/><w>wi</w><w>kṣuṃntsa</w><w>kṣemārcune</w><w>lānti</w><w>mewyai</w><w>pikulne</w><w>ñuñce</w><w>śwer</w><w>meñantse</w><w>-ne</w><w>sāṅk</w><w>śānta</w><w>keś</w>
<lb corresp="#a2"/><w>tessa</w><w>keśa</w><w>postäṃ</w><w>te</w><w>yiknesa</w><w>moko</w><w>ñwenakke</w><pc>•</pc><w>ccākkare</w><pc>•</pc><w>saṅkatāse</w><w>te</w>
<w>yikne<lb corresp="#a3"/>sa</w><w>śānta</w><w>parra</w><w>spāwäṣar</w><pc>•</pc><w>saṅkatāse</w><w>kwañye</w><w>śiṅkrontse</w><w>aṣicce</w><w>alaṃ</w><w>wästa</w>
<w>pku<lb corresp="#a4"/>weṣ</w><w>śaiyye</w><w>plyeṅksa</w><w>wi</w><w>cakaṃnma</w><w>piś</w><w>tom</w><w>yaptsa</w>
<lb corresp="#a5"/><w>saṅkatāse</w><w>lestāñe</w><w>camiläṃntse</w><w>aṣicce</w><w>alaṃ</w><w>wästa</w><w>pkuweṣ</w><w>śaiyye</w><w>plyeṅksa</w><w>wi</w><w>cakaṃnma</w>
<w>pi<lb corresp="#a6"/>ś</w><w>tom</w><w>yyaptsa</w><pc>•</pc><w>saṅkatāse</w><pc>•</pc><w>ccākkare</w><w>hwār</w><w>ppai</w><w>yparwe</w><w>catilentse</w><w>aiyye</w><w>alaṃ</w>
<lb corresp="#a7"/><w>śaiyye</w><w>wsare</w><w>ṣeme</w><w>saṅkatāse</w><w>wäṃṣṣintse</w><w>aiyye</w><w>alaṃ</w><w>pkuweṣ</w>
<w>śai<lb corresp="#a8"/>yye</w><w>wasa</w><w>wi</w><w>cakaṃnma</w><w>śātresa</w><w>ñwenakke</w><w>aiyyāna</w><w>śānta</w><w>wāya</w><w>wi</w>
<lb corresp="#a9"/><w>śkañce</w><w>śāk</w><w>śwerne</w><w>ypay</w><w>moko</w><w>raktakullentse</w>
<w>ccākka<lb corresp="#a10"/>re</w><w>yauyekṣintse</w><w>pakana</w><w>aiyye</w><w>alaṃ</w><w>pkuweṣ</w><w>śaiyye</w><w>wasa</w><w>ṣeme</w><pc>•</pc>
<lb corresp="#a11"/><w>cenak</w><w>śāk</w><w>śwerne</w><w>kapyāri</w><w>śrāy</w><w>klaiyna</w><w>orocce</w><w>kemesa</w><w>asaṃntse</w><w>ṣañ</w><w>ṣārwai</w><w>śśa</w>
