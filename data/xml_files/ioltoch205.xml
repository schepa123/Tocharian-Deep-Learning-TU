               <choice corresp="#a1">
                  <seg><seg type="prim_transl">... 1. || Mīna says: «Dear ...</seg></seg>
               </choice>
               <choice corresp="#a2">
                  <seg><seg type="prim_transl">... of sorrow has arisen in my heart. ...</seg></seg>
               </choice>
               <choice corresp="#a3">
                  <seg><seg type="prim_transl">... king (-)Arjuna to the <hi>rudhiharaka</hi>s ...</seg></seg>
               </choice>
               <choice corresp="#a4">
                  <seg><seg type="prim_transl">... the light is led astray ...</seg></seg>
               </choice>
               <choice corresp="#a5">
                  <seg><seg type="prim_transl">...</seg></seg>
               </choice>
               <choice corresp="#a6">
                  <seg><seg type="prim_transl">...</seg></seg>
               </choice>
               <choice corresp="#b1">
                  <seg><seg type="prim_transl">... ornament ...</seg></seg>
               </choice>
               <choice corresp="#b2">
                  <seg><seg type="prim_transl">... continuously. ...</seg></seg>
               </choice>
               <choice corresp="#b3">
                  <seg><seg type="prim_transl">... Mīna. He causes the prince to go up and says: « ...</seg></seg>
               </choice>
               <choice corresp="#b4">
                  <seg><seg type="prim_transl">... gestures ... Which way ...</seg></seg>
               </choice>
               <choice corresp="#b5">
                  <seg><seg type="prim_transl">... god Kāma has measured (?) the ... of love.</seg></seg>
