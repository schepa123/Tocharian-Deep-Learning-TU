            <p><label>Lines</label><seg corresp="#a1"><bibl><title corresp="ref:thomas1957"/><citedRange>280[f]</citedRange></bibl>; <bibl><title corresp="ref:schmidtkt1974"/><citedRange>50[f]</citedRange></bibl>; <bibl><title corresp="ref:thomas1983"/><citedRange>20</citedRange></bibl>; </seg><seg corresp="#a3"><bibl><title corresp="ref:thomas1957"/><citedRange>228[f]</citedRange></bibl>; <bibl><title corresp="ref:carling2000"/><citedRange>315</citedRange></bibl>; <bibl><title corresp="ref:kolver1965"/><citedRange>116</citedRange></bibl>; </seg><seg corresp="#a4"><bibl><title corresp="ref:thomas1957"/><citedRange>292</citedRange></bibl>; <bibl><title corresp="ref:kolver1965"/><citedRange>127</citedRange></bibl>; </seg><seg corresp="#a5"><bibl><title corresp="ref:thomas1957"/><citedRange>292</citedRange></bibl>; </seg><seg corresp="#a6"><bibl><title corresp="ref:thomas1957"/><citedRange>292</citedRange></bibl>; </seg><seg corresp="#a7"><bibl><title corresp="ref:thomas1957"/><citedRange>292</citedRange></bibl>; </seg><seg corresp="#a8"><bibl><title corresp="ref:thomas1957"/><citedRange>292</citedRange></bibl>; </seg><seg corresp="#b1"><bibl><title corresp="ref:thomas1957"/><citedRange>292</citedRange></bibl>; <bibl><title corresp="ref:carling2000"/><citedRange>234</citedRange></bibl>; <bibl><title corresp="ref:knoll1996"/><citedRange>143, 147</citedRange></bibl>; </seg><seg corresp="#b2"><bibl><title corresp="ref:thomas1957"/><citedRange>292</citedRange></bibl>; </seg><seg corresp="#b3"><bibl><title corresp="ref:thomas1957"/><citedRange>210</citedRange></bibl>. </seg></p>
         </div>
