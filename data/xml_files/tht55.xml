                  <l n="d">
<lb corresp="#a3"/><gap reason="lost"/><w>astarona</w>
<lb corresp="#a4"/><gap reason="lost"/><w>sw</w><w>āñme</w><w>y<gap reason="plain" unit="part"/></w><gap reason="plain" quantity="1"/>
<lb corresp="#a5"/><gap reason="lost"/><w>sa</w><num value="33">30-3</num></l>
