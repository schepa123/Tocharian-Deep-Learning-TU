         <div type="transcription">
            <p>
<lb corresp="#a1"/><gap reason="lost"/><w><gap reason="plain" unit="part"/>t<gap reason="plain" unit="part"/></w><w>m<supplied>ä</supplied>lkwerä</w><w>känte</w><w>ikäṃ</w><gap reason="lost"/>
<lb corresp="#a2"/><gap reason="lost"/><w><supplied>ṣä</supplied>lype</w><w>sāṃtke</w><pc>॥</pc><w>candānä</w><pc>•</pc><w>patma<supplied>kä</supplied></w><gap reason="lost"/>
<lb corresp="#a3"/><gap reason="lost"/><w><supplied>tä</supplied>ryāka</w><w>wi</w><w>traunta</w><w>ṣälype</w><gap reason="lost"/>
<lb corresp="#a4"/><gap reason="lost"/><w><supplied>teka</supplied>nmane</w><w>sāṃtke</w><pc>•</pc><w>leśpäṣ<supplied>ṣ</supplied>e</w><w><supplied>tekine</supplied></w><gap reason="lost"/>
<lb corresp="#a5"/><gap reason="lost"/><gap reason="plain" quantity="1"/><pc>•</pc><w>trayamāna</w><pc>•</pc><w>kuru<supplied>ci</supplied></w><gap reason="lost"/>
<lb corresp="#a6"/><gap reason="lost"/><w><supplied>ṣä</supplied>l<supplied>yp</supplied>e</w><w><supplied>s</supplied>ā<add>ṃ</add>t<supplied>k</supplied>e</w><pc>•</pc><gap reason="lost"/>
