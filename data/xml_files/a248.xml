            <lg type="poem" met="frg:M25" subtype="hom">
               <lg n="20" type="strophe">
                  <l n="a" real="0/5/4-4/4-3">
<lb corresp="#a1"/><caesura ana="frg:cs-s"/><gap reason="plain" quantity="1"/><w>krant</w><w>arämpāt</w><caesura ana="frg:cs-s"/><w>lukśanuntsāṃ</w><caesura ana="frg:cs-w"/><w>kaṣ</w><w>swāñcenyo</w><caesura ana="frg:cs-s"/><w>worpūs</w><w>skam</w><w>ci</w><caesura ana="frg:cs-w"/><w>kapśañi</w><pc>:</pc></l>
