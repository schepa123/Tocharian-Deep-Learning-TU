<lb corresp="#a3"/><gap reason="plain" quantity="3"/><caesura ana="frg:cs-s"/><gap reason="plain" quantity="4"/><caesura ana="frg:cs-w"/><w>lakṣañi</w><w>tñi</w><caesura ana="frg:cs-s"/><w>oktuk</w><w>śkaṃ</w><w>tsnont</w><caesura ana="frg:cs-w"/><w>yetweyntu</w><pc>:</pc></l>
                  <l n="d"><w>poñcn</w><w>ārkiśoṣṣis</w><caesura ana="frg:cs-s"/><w>lkālu</w>
<lb corresp="#a4"/><gap reason="plain" quantity="3"/><caesura ana="frg:cs-s"/><gap reason="plain" quantity="4"/><caesura ana="frg:cs-w"/><gap reason="plain" quantity="2"/><w>r</w><w>nāṃtsunt</w><caesura ana="frg:cs-s"/><w>wināsam</w><w>cū</w><caesura ana="frg:cs-w"/><w>mrāc</w><w>śpālyo</w><num value="20">20</num></l>
