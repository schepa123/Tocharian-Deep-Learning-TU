         <div type="translation">
            <p>
               <choice corresp="#a1">
                  <seg><seg type="prim_transl">...</seg></seg>
               </choice>
               <choice corresp="#a2">
                  <seg><seg type="prim_transl">... for salvation ... who is totally able ... </seg></seg>
               </choice>
               <choice corresp="#a3">
                  <seg><seg type="prim_transl">... in the Udānālaṅkāra ...</seg></seg>
               </choice>
               <choice corresp="#a4">
                  <seg><seg type="prim_transl">... as long as faith ...</seg></seg>
               </choice>
               <choice corresp="#a5">
                  <seg><seg type="prim_transl">... he is able to give (a gift) ...</seg></seg>
               </choice>
               <choice corresp="#a6">
                  <seg><seg type="prim_transl">...</seg></seg>
               </choice>
               <choice corresp="#b2">
                  <seg><seg type="prim_transl">... </seg></seg>
               </choice>
               <choice corresp="#b3">
                  <seg><seg type="prim_transl">... away ...</seg></seg>
               </choice>
               <choice corresp="#b4">
                  <seg><seg type="prim_transl">... other ...</seg></seg>
               </choice>
               <choice corresp="#b5">
                  <seg><seg type="prim_transl">... harsh saying now ...</seg></seg>
               </choice>
               <choice corresp="#b6">
                  <seg><seg type="prim_transl">... births five ...</seg></seg>
               </choice>
            </p>
         </div>
         <div type="comments">
