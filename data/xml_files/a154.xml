         <div type="transcription">
            <p><milestone unit="passage" n="Famine"/>
<lb corresp="#a1"/><gap reason="lost"/><w>r<gap reason="plain" unit="part"/>s</w><w>y<gap reason="plain" unit="part"/></w><gap reason="plain" quantity="2"/><w>ñc<gap reason="plain" unit="part"/></w><w>tsru</w><gap reason="lost"/>
<lb corresp="#a2"/><gap reason="lost"/><w>s</w><w>sāsepus</w><w>tāṣ</w><w>tsru</w><w>ats</w><w>wacont</w><w>śwātsi</w><w>tā</w><gap reason="lost"/>
<lb corresp="#a3"/><gap reason="lost"/><w><gap reason="plain" unit="part"/></w><w>itaṃ</w><w>nāṃtsus</w><pc>•</pc><w>mäṃt</w><w>ne</w><w>ālyäkyāṃ</w><w>praṣtaṃ</w><w>kaṣt</w><w>ṣeṣ</w><w>sukrānāñ</w><w><gap reason="plain" unit="part"/></w><w>ynā</w><gap reason="lost"/>
<lb corresp="#a4"/><gap reason="lost"/><w>wats</w><w>tmäṣ</w><w>tom</w><w>wrināñ</w><w>lwā</w><w>śākant</w><w>sukrānāśśi</w><w>śwātsi</w><w>eṣār</w><pc>•</pc><w>pra</w><gap reason="lost"/>
<lb corresp="#a5"/><gap reason="lost"/><w>tmäṣ</w><w>tom</w><w>wālätkāñ</w><w>kaṣtyo</w><w>miṣānt</w><w>tosäm</w><w>sukrānāsac</w><w>weñā<supplied>r</supplied></w><gap reason="lost"/>
<lb corresp="#a6"/><gap reason="ink_lost"/>
<lb corresp="#b1"/><gap reason="ink_lost"/>
<lb corresp="#b2"/><gap reason="lost"/><w><supplied>wla</supplied>lune</w><w>ākatsum</w><w>paku</w><w>oko</w><w>oki</w><w>skamat</w><w>klāluneyā</w><w>praskär</w><gap reason="lost"/>
<lb corresp="#b3"/><gap reason="lost"/><w>śāyu</w><w>oki</w><w>sne</w><w>waste</w><w>nāṃtsu</w><pc>•</pc><pc>॥</pc><w>maitraṃ</w><pc>॥</pc></p>
            <lg type="poem" met="frg:M12" subtype="hom">
               <lg n="1" type="strophe">
                  <l n="a"><w>mā</w><w>ñkät</w><w>mā</w><w>napeṃ</w><caesura ana="frg:cs-s"/><w>mā</w><w>penu</w><w>śāt</w><caesura ana="frg:cs-w"/><w>mā</w><gap reason="lost"/><supplied unit="syllable" quantity="2"/></l>
