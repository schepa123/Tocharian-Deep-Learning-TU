<lb corresp="#b3"/><gap reason="lost"/><gap reason="plain" quantity="1"/><w>nt</w><num value="16">10-6</num><w><supplied>kU</supplied>s<supplied>e</supplied></w><w>swañcaiṃ</w><w>mā</w><gap reason="lost"/>
