               </lg>
               <lg n="7" type="strophe">
                  <l n="d" real="30"><w>kUse</w><w>ñ<supplied>a</supplied>k<supplied>e</supplied></w><gap reason="lost"/>
<lb corresp="#a3"/><gap reason="lost"/><w>r</w><w>ce</w><w>somśke</w><w>lare</w><gap reason="plain" quantity="3"/><w>ts</w><w>sokta</w><gap reason="lost"/>
