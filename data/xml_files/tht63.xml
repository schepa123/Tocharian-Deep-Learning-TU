                  <l n="x" real="7"><gap reason="lost"/>
<lb corresp="#b6"/><gap reason="lost"/><w>po</w><w>yo</w><gap reason="plain" quantity="3"/><w>m<gap reason="plain" unit="part"/></w><w>skau</w><pc>:</pc></l>
