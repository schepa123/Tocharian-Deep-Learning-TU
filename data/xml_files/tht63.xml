            <p><milestone unit="passage" n="uncertain"/></p>
            <lg type="poem" met="frg:M20_22_10_15" subtype="hom">
               <lg n="X" type="strophe">
                  <l n="x" real="3">
<lb corresp="#a1"/><gap reason="lost"/><w>te<supplied>mtsa</supplied>te</w><pc>:</pc></l>
                  <l n="x" real="4-4/4-3/4-3"><w>śem-neś</w><w>śerwe</w><w>kautsi</w><w>weñi</w><caesura ana="frg:cs-s"/><w>so<gap reason="plain" unit="part"/><gap reason="plain" unit="part"/>n<gap reason="plain" unit="part"/></w><gap reason="lost"/>
