            <lg type="poem" met="frg:M21_21_18_13" subtype="hom">
               <lg n="1" type="strophe">
                  <l n="a"><w>sne</w>
<lb corresp="#a3"/><supplied unit="syllable" quantity="19"/><gap reason="lost"/><w>śäl</w><pc>:</pc></l>
                  <l n="b"><w>kUpre</w><w>ne</w><w>cesmi</w><w>cam</w><caesura ana="frg:cs-w"/><w>eṃts</w><w>yäslyi</w><caesura ana="frg:cs-s"/><w>bodhisatvāñ</w><caesura ana="frg:cs-w"/><w>pāṣlye</w><w>ṣe<milestone unit="w" type="break"/></w>
<lb corresp="#a4"/><gap reason="plain" quantity="1"/><supplied unit="syllable" quantity="0"/><caesura ana="frg:cs-w"/><supplied unit="syllable" quantity="6"/></l>
