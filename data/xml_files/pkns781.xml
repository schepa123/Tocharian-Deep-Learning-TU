<lb corresp="#a4"/><w>-rbhaśāṅk</w><w>spharir</w><w>se</w><w>te<milestone unit="w" type="break"/></w><gap reason="lost"/>
<lb corresp="#a5"/><w><gap reason="plain" unit="part"/>s<gap reason="plain" unit="part"/></w><w>ñī</w><w>prārñaṃ</w><w>swarnasūtär</w><w><gap reason="plain" unit="part"/><gap reason="plain" unit="part"/></w><gap reason="lost"/>
<lb corresp="#a6"/><w>ṣai</w><w>wīktañ<gap reason="plain" unit="part"/>ä</w><w><gap reason="plain" unit="part"/>e</w><w>r<gap reason="plain" unit="part"/><gap reason="plain" unit="part"/></w><gap reason="lost"/>
